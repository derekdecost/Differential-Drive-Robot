<launch>
  <include file="$(find mystery_package)/launch/launch_mystery.launch"/>

  <group ns="homework_4">
    <param name="/hw4/coversion_type" type="string" value="meter"/>
    <!--The ROS node created in HW4-->
    <node pkg="ros_pkg_hw2" name="hw4_node" type="hw4_node.py"/>
    <!--Remap the input node of the HW2 node to listen to the output node of HW4-->
    <remap from="/mystery/output1" to="/hw4/hw4_output"/>
    <node pkg="ros_pkg_hw2" name="hw2_node" type="hw2_node.py"/>
  </group>
</launch>
